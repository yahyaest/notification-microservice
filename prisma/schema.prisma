generator client {
  provider  = "prisma-client-py"
  // config_option = "value"
  interface = "asyncio"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Notification {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  userEmail String?
  username  String
  userImage String?
  userId    Int?
  title     String
  message   String
  sender    String?
  seen      Boolean  @default(false)
}
